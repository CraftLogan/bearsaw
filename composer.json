{
    "name": "fwartner/bearsaw",
    "description": "Sync your bear notes with jigsaw",
    "keywords": ["bear", "jigsaw", "laravel zero", "blog", "markdown"],
    "homepage": "https://wartner.io",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/fwartner/bearsaw/issues",
        "source": "https://github.com/fwartner/bearsaw"
    },
    "authors": [
        {
            "name": "Florian Wartner",
            "email": "florian@wartner.io"
        }
    ],
    "require": {
        "php": "^7.2",
        "calebporzio/bear-sync": "^1.0",
        "laravel-zero/framework": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["bearsaw"]
}
